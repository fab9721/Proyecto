A,MAAA,CAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAvBA;AAyBA,IAAA,mBAzBA,iCAyBA;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,KAAA,WAAA,EAAA,CAAA;AACA,WAAA,WAAA;AACA,KA5BA;AA8BA,IAAA,KA9BA,mBA8BA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,aAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KApCA;AAsCA,IAAA,WAtCA,yBAsCA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,aAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KA5CA;AA8CA,IAAA,IA9CA,kBA8CA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,OAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,YAAA;AACA,OAFA,MAEA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,KAAA,YAAA;AACA;;AACA,WAAA,KAAA;AACA;AArDA;AAhDA,CAAA","sourcesContent":["<template>\r\n    <v-data-table\r\n          :headers=\"headers\"\r\n          :items=\"bodegas\"\r\n          sort-by=\"descripcion\"\r\n          class=\"elevation-1\"\r\n        >\r\n        <template v-slot:top>\r\n        <v-toolbar\r\n          flat\r\n        >\r\n        <v-toolbar-title>Listado de Bodegas</v-toolbar-title>\r\n        <v-divider\r\n          class=\"mx-4\"\r\n          inset\r\n          vertical\r\n        ></v-divider>\r\n        <v-spacer></v-spacer>\r\n        <v-dialog\r\n          v-model=\"dialog\"\r\n          max-width=\"500px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn\r\n              color=\"primary\"\r\n              dark\r\n              class=\"mb-2\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n              to=\"/bodegas/crear\"\r\n            >\r\n              Nuevo\r\n            </v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedBodega.bodega_id\"\r\n                      label=\"ID\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedBodega.codigo\"\r\n                      label=\"Codigo\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedBodega.descripcion\"\r\n                      label=\"Descripcion\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn\r\n                color=\"blue darken-1\"\r\n                text\r\n                @click=\"close\"\r\n              >\r\n                Cancelar\r\n              </v-btn>\r\n              <v-btn\r\n                color=\"blue darken-1\"\r\n                text\r\n                @click=\"save\"\r\n              >\r\n                Guardar\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Esta seguro de eliminar este registro?</v-card-title>\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancelar</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"deleteBodegaConfirm\">OK</v-btn>\r\n              <v-spacer></v-spacer>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:[`item.actions`]=\"{ item }\">\r\n      <v-icon\r\n        small\r\n        color=\"blue darken-1\"\r\n        class=\"mr-2\"\r\n        @click=\"editBodega(item)\"\r\n      >\r\n        mdi-pencil\r\n      </v-icon>\r\n      <v-icon\r\n        small\r\n        color=\"blue darken-1\"\r\n        @click=\"deleteBodega(item)\"\r\n      >\r\n        mdi-delete\r\n      </v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn\r\n        color=\"primary\"\r\n        @click=\"initialize\"\r\n      >\r\n        Reiniciar\r\n      </v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n\r\n<script>\r\nlet url = 'http://localhost:3000/bodegas/'\r\n//let articulos;\r\nimport axios from 'axios';\r\n  export default {\r\n    data: () => ({\r\n      dialog: false,\r\n      dialogDelete: false,\r\n      headers: [\r\n        {\r\n          text: 'ID',\r\n          align: 'start',\r\n          sortable: false,\r\n          value: 'bodega_id',\r\n        },\r\n        { text: 'Codigo', value: 'codigo' },\r\n        { text: 'Descripcion', value: 'descripcion' },\r\n        { text: 'Actions', value: 'actions', sortable: false },\r\n      ],\r\n      bodegas: [],\r\n      editedIndex: -1,\r\n      editedBodega: {\r\n        bodega_id:0,\r\n        codigo: '',\r\n        descripcion: ''\r\n      },\r\n      defaultBodega: {\r\n        bodega_id:0,\r\n        codigo: '',\r\n        descripcion: ''\r\n      },\r\n    }),\r\n\r\n    computed: {\r\n      formTitle () {\r\n        return this.editedIndex === -1 ? 'Nueva Bodega' : 'Editar Bodega'\r\n      },\r\n    },\r\n\r\n    watch: {\r\n      dialog (val) {\r\n        val || this.close()\r\n      },\r\n      dialogDelete (val) {\r\n        val || this.closeDelete()\r\n      },\r\n    },\r\n\r\n    created () {\r\n      this.initialize()\r\n    },\r\n\r\n    methods: {\r\n      initialize () {\r\n        axios\r\n            .get(url)\r\n            .then(response=>{\r\n              this.bodegas = response.data.bodega;\r\n              console.log(this.bodegas);\r\n            }).catch((error)=>{\r\n                console.log(error);\r\n            })\r\n      },\r\n\r\n      editBodega (bodega) {\r\n        this.editedIndex = this.bodegas.indexOf(bodega)\r\n        this.editedBodega = Object.assign({}, bodega)\r\n        this.$router.push({name: 'EditarBodegas',params:{id:bodega.bodega_id}})\r\n        //this.dialog = true\r\n      },\r\n\r\n      deleteBodega (bodega) {\r\n        this.editedIndex = this.bodegas.indexOf(bodega)\r\n        this.editedBodega = Object.assign({}, bodega)\r\n        this.dialogDelete = true\r\n      },\r\n\r\n      deleteBodegaConfirm () {\r\n        this.bodegas.splice(this.editedIndex, 1)\r\n        this.closeDelete()\r\n      },\r\n\r\n      close () {\r\n        this.dialog = false\r\n        this.$nextTick(() => {\r\n          this.editedBodega = Object.assign({}, this.defaultBodega)\r\n          this.editedIndex = -1\r\n        })\r\n      },\r\n\r\n      closeDelete () {\r\n        this.dialogDelete = false\r\n        this.$nextTick(() => {\r\n          this.editedBodega = Object.assign({}, this.defaultBodega)\r\n          this.editedIndex = -1\r\n        })\r\n      },\r\n\r\n      save () {\r\n        if (this.editedIndex > -1) {\r\n          Object.assign(this.bodegas[this.editedIndex], this.editedBodega)\r\n        } else {\r\n          this.bodegas.push(this.editedBodega)\r\n        }\r\n        this.close()\r\n      },\r\n    },\r\n  }\r\n</script>"],"sourceRoot":"src/views/bodegas"}]}