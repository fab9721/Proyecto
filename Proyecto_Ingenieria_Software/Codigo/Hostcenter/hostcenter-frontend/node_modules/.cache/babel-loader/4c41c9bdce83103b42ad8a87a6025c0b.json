e"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuHA,IAAA,GAAA,GAAA,iCAAA;AACA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,MAAA,MAAA,QAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,eADA;AAEA,EAAA,OAFA,qBAEA;AAAA;;AACA;AACA,SAAA,UAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CAFA,CAGA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,KAAA,UAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,CAAA,CADA,CAEA;AACA;AACA;AACA,KANA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,4BAAA,KAAA;AACA,KARA;AASA,GAfA;AAgBA,EAAA,IAhBA,kBAgBA;AACA,WAAA;AACA,MAAA,UAAA,EAAA,IADA;AAEA,MAAA,OAAA,EAAA;AACA,QAAA,mBAAA,EAAA,EADA;AAEA,QAAA,cAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA,EAHA;AAIA,QAAA,SAAA,EAAA,EAJA;AAKA,QAAA,SAAA,EAAA,EALA;AAMA,QAAA,QAAA,EAAA,EANA;AAOA,QAAA,KAAA,EAAA,EAPA;AAQA,QAAA,gBAAA,EAAA,IARA;AASA,QAAA,SAAA,EAAA,CATA;AAUA,QAAA,WAAA,EAAA;AAVA,OAFA;AAcA,MAAA,IAAA,EAAA,CACA;AAAA,QAAA,IAAA,EAAA,QAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAFA,CAdA;AAkBA,MAAA,IAAA,EAAA,KAlBA,CAmBA;;AAnBA,KAAA;AAqBA,GAtCA;AAuCA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,UAAA,MAAA,GAAA,KAAA,OAAA,CADA,CAEA;;AACA,WAAA,OAAA,CAAA,gBAAA,GAAA,MAAA,CAAA,KAAA,OAAA,CAAA,gBAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA,CAHA,CAIA;;AACA,UAAA,MAAA,GAAA,KAAA,OAAA,CALA,CAMA;AACA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,KAAA,UAAA,EAAA,MAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA,KAhBA;AAiBA,IAAA,UAjBA,sBAiBA,KAjBA,EAiBA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA;AAnBA;AAvCA,CAAA","sourcesContent":["<template>\r\n    <v-container>\r\n        <v-row>\r\n            <v-col class=\"mb-4\">\r\n                <h1 class=\"display-2 font-weight-bold-mb-3\">Edicion de personas</h1>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col>\r\n                <form v-on:submit.prevent=\"guardarPersona()\">\r\n                    <v-select\r\n                        v-model=\"persona.tipo_identificacion\"\r\n                        :items=\"tipo\"\r\n                        item-value=\"value\"\r\n                        item-text=\"text\"\r\n                        label=\"Tipo\"\r\n                    ></v-select>\r\n                    <v-text-field \r\n                        v-model=\"persona.identificacion\"\r\n                        label=\"Identificacion\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.nombres\"\r\n                        label=\"Nombres\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.apellidos\"\r\n                        label=\"Apellidos\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.direccion\"\r\n                        label=\"Direccion\"\r\n                        outlined\r\n                        required\r\n                    ></v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.telefono\"\r\n                        label=\"Telefono\"\r\n                        outlined\r\n                        type=\"number\"\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.email\"\r\n                        label=\"Email\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-menu\r\n                        ref=\"menu\"\r\n                        v-model=\"menu\"\r\n                        :close-on-content-click=\"false\"\r\n                        :return-value.sync=\"persona.fecha_nacimiento\"\r\n                        transition=\"scale-transition\"\r\n                        offset-y\r\n                        min-width=\"auto\"\r\n                    >\r\n                        <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field\r\n                            \r\n                            :value=\"formatDate(persona.fecha_nacimiento)\"\r\n                            label=\"Fecha Nacimiento\"\r\n                            prepend-icon=\"mdi-calendar\"\r\n                            readonly\r\n                            v-bind=\"attrs\"\r\n                            v-on=\"on\"\r\n                        ></v-text-field>\r\n                        </template>\r\n                        <v-date-picker\r\n                        v-model=\"persona.fecha_nacimiento\"\r\n                        no-title\r\n                        scrollable\r\n                        >\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            text\r\n                            color=\"primary\"\r\n                            @click=\"menu = false\"\r\n                        >\r\n                            Cancel\r\n                        </v-btn>\r\n                        <v-btn\r\n                            text\r\n                            color=\"primary\"\r\n                            @click=\"$refs.menu.save(persona.fecha_nacimiento)\"\r\n                        >\r\n                            OK\r\n                        </v-btn>\r\n                        </v-date-picker>\r\n                    </v-menu>\r\n                    <v-checkbox\r\n                        v-model=\"persona.esCliente\"\r\n                        label=\"Es Cliente\"\r\n                    ></v-checkbox>\r\n                    <v-checkbox\r\n                        v-model=\"persona.esProveedor\"\r\n                        label=\"Es Proveedor\"\r\n                    ></v-checkbox>\r\n                    <v-card-actions>\r\n                        <v-btn to=\"/personas\" color=\"blue-grey\" class=\"mr-4\">Cancelar</v-btn>\r\n                        <v-btn type=\"submit\" color=\"indigo\" class=\"mr-4\">Guardar</v-btn>\r\n                    </v-card-actions>\r\n                </form>\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n<script>\r\nlet url = 'http://localhost:3000/personas/'\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nexport default {\r\n    name:'EditarPersona',\r\n    mounted(){\r\n        //OBTENGO TODOS LOS DATOS\r\n        this.persona_id=this.$route.params.id\r\n        //console.log(this.producto_id);\r\n        axios.get(url+this.persona_id)\r\n        .then(response=>{\r\n            this.persona=response.data.persona[0]\r\n            //persona.fecha_nacimiento = moment(persona.fecha_nacimiento).format('L')\r\n            //console.log(\"Datos\")\r\n            //console.log(response.data.data[0]);\r\n        }).catch((error)=>{\r\n            console.log(\"El error de la peticion\"+error);\r\n        });\r\n    },\r\n    data(){\r\n        return{\r\n            persona_id:null,\r\n            persona:{\r\n                tipo_identificacion:'',\r\n                identificacion:'',\r\n                nombres:'',\r\n                apellidos:'',\r\n                direccion:'',\r\n                telefono:'',\r\n                email:'',\r\n                fecha_nacimiento: null,\r\n                esCliente:0,\r\n                esProveedor:0,\r\n            },\r\n            tipo:[\r\n                {text:'Cedula',value:'C'},\r\n                {text:'Ruc',value:'R'}\r\n            ],\r\n            menu: false,\r\n            //fecha:moment(persona.fecha_nacimiento).format('L')\r\n        }\r\n    },\r\n    methods:{\r\n        guardarPersona(){\r\n            let router = this.$router;\r\n            //console.log(router);\r\n            this.persona.fecha_nacimiento = moment(this.persona.fecha_nacimiento).format(\"YYYY-MM-DD\")\r\n            //console.log(this.persona.fecha_nacimiento)\r\n            let params = this.persona;\r\n            //console.log(params);\r\n            //console.log(url+this.persona_id)\r\n            axios.put(url+this.persona_id,params)\r\n            .then(()=>{\r\n                router.push('/personas');\r\n            })\r\n            .catch((error)=>{\r\n                console.log(error);\r\n            });\r\n        },\r\n        formatDate(value) {\r\n            return moment(value).format(\"YYYY-MM-DD\")\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/views/personas"}]}