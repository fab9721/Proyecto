e"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA,IAAA,GAAA,GAAA,6BAAA;AACA,IAAA,cAAA,GAAA,wCAAA;AACA,OAAA,KAAA,MAAA,OAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,eADA;AAEA,EAAA,OAFA,qBAEA;AAAA;;AAEA;AACA,SAAA,oBAAA,GAHA,CAKA;;AACA,SAAA,UAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CANA,CAOA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,KAAA,UAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA,KAHA,EAGA,KAHA,CAGA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,4BAAA,KAAA;AACA,KALA;AAMA,GAhBA;AAiBA,EAAA,IAjBA,kBAiBA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,KADA;AAEA,MAAA,KAAA,EAAA;AACA,QAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,iBAAA,CAAA,CAAA,KAAA,IAAA,YAAA;AAAA,SADA;AAEA,QAAA,GAAA,EAAA,aAAA,CAAA;AAAA,iBAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,qBAAA;AAAA;AAFA,OAFA;AAMA,MAAA,UAAA,EAAA,IANA;AAOA,MAAA,OAAA,EAAA;AACA,QAAA,eAAA,EAAA,CADA;AAEA,QAAA,OAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA;AAHA,OAPA;AAYA,MAAA,WAAA,EAAA;AAZA,KAAA;AAcA,GAhCA;AAiCA,EAAA,OAAA,EAAA;AACA,IAAA,oBADA,kCACA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,cAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA;AACA,OAJA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA,KATA;AAUA,IAAA,cAVA,4BAUA;AACA,UAAA,MAAA,GAAA,KAAA,OAAA;AACA,UAAA,MAAA,GAAA,KAAA,OAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,KAAA,UAAA,EAAA,MAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA;AApBA;AAjCA,CAAA","sourcesContent":["<template>\r\n    <v-container>\r\n        <v-row>\r\n            <v-col class=\"mb-4\">\r\n                <h1 class=\"display-2 font-weight-bold-mb-3\">Edicion de Usuarios</h1>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col>\r\n                <form v-on:submit.prevent=\"guardarUsuario()\">\r\n                    <v-text-field \r\n                        v-model=\"usuario.codigo\"\r\n                        label=\"Codigo\"\r\n                        outlined\r\n                        readonly\r\n                    >\r\n                    </v-text-field>\r\n                    <v-select\r\n                        v-model=\"usuario.tipo_usuario_id\"\r\n                        item-value=\"tipo_usuario_id\"\r\n                        item-text=\"descripcion\"\r\n                        :items = \"tipousuario\"\r\n                        label=\"Tipo Usuario\"\r\n                        required\r\n                    >\r\n                    </v-select> \r\n                    <v-text-field \r\n                        v-model=\"usuario.nombres\"\r\n                        label=\"Nombres\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field\r\n                        v-model=\"usuario.clave\"\r\n                        :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                        :rules=\"[rules.required, rules.min]\"\r\n                        :type=\"show1 ? 'text' : 'password'\"\r\n                        name=\"input-10-1\"\r\n                        label=\"Clave\"\r\n                        hint=\"Minimo 8 caracteres\"\r\n                        counter\r\n                        @click:append=\"show1 = !show1\"\r\n                    ></v-text-field>\r\n                    <v-card-actions>\r\n                        <v-btn to=\"/usuarios\" color=\"blue-grey\" class=\"mr-4\">Cancelar</v-btn>\r\n                        <v-btn type=\"submit\" color=\"indigo\" class=\"mr-4\">Guardar</v-btn>\r\n                    </v-card-actions>\r\n                </form>\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n<script>\r\nlet url = 'http://localhost:3000/user/'\r\nlet urlTipoUsuario = 'http://localhost:3000/tipousuario/all/'\r\nimport axios from 'axios';\r\nexport default {\r\n    name:'EditarUsuario',\r\n    mounted(){\r\n\r\n         //OBTENGO LOS TIPOS DE USUARIOS\r\n        this.obtenerTiposUsuarios();\r\n\r\n        //OBTENGO TODOS LOS DATOS\r\n        this.usuario_id=this.$route.params.id\r\n        //console.log(this.producto_id);\r\n        axios.get(url+this.usuario_id)\r\n        .then(response=>{\r\n            this.usuario=response.data.usuario[0]\r\n        }).catch((error)=>{\r\n            console.log(\"El error de la peticion\"+error);\r\n        });\r\n    },\r\n    data(){\r\n        return{\r\n            show1: false,\r\n            rules: {\r\n                required: value => !!value || 'Requerido.',\r\n                min: v => v.length >= 8 || 'Minimo 8 caracteres',\r\n            },\r\n            usuario_id:null,\r\n            usuario:{\r\n                tipo_usuario_id:0,\r\n                nombres:'',\r\n                clave: ''\r\n            },\r\n            tipousuario:[],\r\n        }\r\n    },\r\n    methods:{\r\n        obtenerTiposUsuarios(){\r\n            axios.get(urlTipoUsuario)\r\n            .then(response=>{\r\n                this.tipousuario = response.data.data;\r\n                console.log(this.tipousuario);\r\n            }).catch((error)=>{\r\n                console.log(error);\r\n            })\r\n        },\r\n        guardarUsuario(){\r\n            let router = this.$router;\r\n            let params = this.usuario;\r\n            axios.put(url+this.usuario_id,params)\r\n            .then(()=>{\r\n                router.push('/usuarios');\r\n            })\r\n            .catch((error)=>{\r\n                console.log(error);\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/views/usuarios"}]}