A,KAjBA;AAmBA,IAAA,UAnBA,sBAmBA,IAnBA,EAmBA;AACA,WAAA,WAAA,GAAA,KAAA,WAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAvBA;AAyBA,IAAA,iBAzBA,+BAyBA;AAAA;;AACA,UAAA,EAAA,GAAA,KAAA,WAAA,CAAA,KAAA,WAAA,EAAA,wBAAA;AACA,WAAA,WAAA,CAAA,MAAA,CAAA,KAAA,WAAA,EAAA,CAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,GAAA,GAAA,EAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OAHA;AAIA,WAAA,WAAA;AACA,KAjCA;AAmCA,IAAA,KAnCA,mBAmCA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAzCA;AA2CA,IAAA,WA3CA,yBA2CA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAjDA;AAmDA,IAAA,IAnDA,kBAmDA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,WAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,WAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA;;AACA,WAAA,KAAA;AACA;AA1DA;AAnDA,CAAA","sourcesContent":["<template>\r\n    <v-data-table\r\n          :headers=\"headers\"\r\n          :items=\"inventarios\"\r\n          sort-by=\"fecha\"\r\n          class=\"elevation-1\"\r\n        >\r\n        <template v-slot:top>\r\n        <v-toolbar\r\n          flat\r\n        >\r\n        <v-toolbar-title>Listado de Movimientos de Inventario</v-toolbar-title>\r\n        <v-divider\r\n          class=\"mx-4\"\r\n          inset\r\n          vertical\r\n        ></v-divider>\r\n        <v-spacer></v-spacer>\r\n        <v-dialog\r\n          v-model=\"dialog\"\r\n          max-width=\"500px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn\r\n              color=\"primary\"\r\n              dark\r\n              class=\"mb-2\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n              to=\"/inventarios/crear\"\r\n            >\r\n              Nuevo\r\n            </v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.movimiento_inventario_id\"\r\n                      label=\"ID\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.fecha\"\r\n                      label=\"Fecha\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.bodega\"\r\n                      label=\"Bodega\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col\r\n                    cols=\"12\"\r\n                    sm=\"6\"\r\n                    md=\"4\"\r\n                  >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.tipo\"\r\n                      label=\"Tipo\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn\r\n                color=\"blue darken-1\"\r\n                text\r\n                @click=\"close\"\r\n              >\r\n                Cancelar\r\n              </v-btn>\r\n              <v-btn\r\n                color=\"blue darken-1\"\r\n                text\r\n                @click=\"save\"\r\n              >\r\n                Guardar\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Esta seguro de eliminar este registro?</v-card-title>\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Regresar</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">OK</v-btn>\r\n              <v-spacer></v-spacer>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:[`item.actions`]=\"{ item }\">\r\n      <v-icon\r\n        small\r\n        color=\"blue darken-1\"\r\n        class=\"mr-2\"\r\n        @click=\"editItem(item)\"\r\n      >\r\n        mdi-eye\r\n      </v-icon>\r\n      <v-icon\r\n        small\r\n        color=\"blue darken-1\"\r\n        @click=\"deleteItem(item)\"\r\n      >\r\n        mdi-delete\r\n      </v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn\r\n        color=\"primary\"\r\n        @click=\"initialize\"\r\n      >\r\n        Reiniciar\r\n      </v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n\r\n<script>\r\nlet url = 'http://localhost:3000/inventarios/'\r\n//let articulos;\r\nimport axios from 'axios';\r\n  export default {\r\n    data: () => ({\r\n      dialog: false,\r\n      dialogDelete: false,\r\n      headers: [\r\n        {\r\n          text: 'ID',\r\n          align: 'start',\r\n          sortable: false,\r\n          value: 'movimiento_inventario_id',\r\n        },\r\n        { text: 'Fecha', value: 'fecha' },\r\n        { text: 'Bodega', value: 'bodega' },\r\n        { text: 'Tipo', value: 'tipo' },\r\n        { text: 'Actions', value: 'actions', sortable: false },\r\n      ],\r\n      inventarios: [],\r\n      editedIndex: -1,\r\n      editedItem: {\r\n        movimiento_inventario_id:0,\r\n        fecha: '',\r\n        bodega: '',\r\n        tipo: '',\r\n      },\r\n      defaultItem: {\r\n        movimiento_inventario_id:0,\r\n        fecha: '',\r\n        bodega: '',\r\n        tipo: '',\r\n      },\r\n    }),\r\n\r\n    computed: {\r\n      formTitle () {\r\n        return this.editedIndex === -1 ? 'Nuevo Inventario' : 'Editar Inventario'\r\n      },\r\n    },\r\n\r\n    watch: {\r\n      dialog (val) {\r\n        val || this.close()\r\n      },\r\n      dialogDelete (val) {\r\n        val || this.closeDelete()\r\n      },\r\n    },\r\n\r\n    created () {\r\n      this.initialize()\r\n    },\r\n\r\n    methods: {\r\n      initialize () {\r\n        axios\r\n            .get(url)\r\n            .then(response=>{\r\n              this.inventarios = response.data.inventario;\r\n              console.log(this.inventarios);\r\n            }).catch((error)=>{\r\n                console.log(error);\r\n            })\r\n      },\r\n\r\n      editItem (item) {\r\n        this.editedIndex = this.inventarios.indexOf(item)\r\n        this.editedItem = Object.assign({}, item)\r\n        this.$router.push({name: 'VerInventarios',params:{id:item.movimiento_inventario_id}})\r\n        //this.dialog = true\r\n      },\r\n\r\n      deleteItem (item) {\r\n        this.editedIndex = this.inventarios.indexOf(item)\r\n        this.editedItem = Object.assign({}, item)\r\n        this.dialogDelete = true\r\n      },\r\n\r\n      deleteItemConfirm () {\r\n        let id = this.inventarios[this.editedIndex].movimiento_inventario_id;\r\n        this.inventarios.splice(this.editedIndex, 1)\r\n        axios.delete(url+id)\r\n        .then(()=>{\r\n          this.initialize()\r\n        })\r\n        this.closeDelete()\r\n      },\r\n\r\n      close () {\r\n        this.dialog = false\r\n        this.$nextTick(() => {\r\n          this.editedItem = Object.assign({}, this.defaultItem)\r\n          this.editedIndex = -1\r\n        })\r\n      },\r\n\r\n      closeDelete () {\r\n        this.dialogDelete = false\r\n        this.$nextTick(() => {\r\n          this.editedItem = Object.assign({}, this.defaultItem)\r\n          this.editedIndex = -1\r\n        })\r\n      },\r\n\r\n      save () {\r\n        if (this.editedIndex > -1) {\r\n          Object.assign(this.inventarios[this.editedIndex], this.editedItem)\r\n        } else {\r\n          this.inventarios.push(this.editedItem)\r\n        }\r\n        this.close()\r\n      },\r\n    },\r\n  }\r\n</script>"],"sourceRoot":"src/views/inventarios"}]}