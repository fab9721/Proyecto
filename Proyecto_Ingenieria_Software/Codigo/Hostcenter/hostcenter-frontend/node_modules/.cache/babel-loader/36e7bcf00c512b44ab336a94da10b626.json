a.identificacion\"\r\n                        label=\"Identificacion\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.nombres\"\r\n                        label=\"Nombres\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.apellidos\"\r\n                        label=\"Apellidos\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.direccion\"\r\n                        label=\"Direccion\"\r\n                        outlined\r\n                        required\r\n                    ></v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.telefono\"\r\n                        label=\"Telefono\"\r\n                        outlined\r\n                        type=\"number\"\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"persona.email\"\r\n                        label=\"Email\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-menu\r\n                        ref=\"menu\"\r\n                        v-model=\"menu\"\r\n                        :close-on-content-click=\"false\"\r\n                        :return-value.sync=\"persona.fecha_nacimiento\"\r\n                        transition=\"scale-transition\"\r\n                        offset-y\r\n                        min-width=\"auto\"\r\n                    >\r\n                        <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field\r\n                            v-model=\"persona.fecha_nacimiento\"\r\n                            label=\"Fecha Nacimiento\"\r\n                            prepend-icon=\"mdi-calendar\"\r\n                            readonly\r\n                            v-bind=\"attrs\"\r\n                            v-on=\"on\"\r\n                        ></v-text-field>\r\n                        </template>\r\n                        <v-date-picker\r\n                        v-model=\"persona.fecha_nacimiento\"\r\n                        no-title\r\n                        scrollable\r\n                        >\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            text\r\n                            color=\"primary\"\r\n                            @click=\"menu = false\"\r\n                        >\r\n                            Cancel\r\n                        </v-btn>\r\n                        <v-btn\r\n                            text\r\n                            color=\"primary\"\r\n                            @click=\"$refs.menu.save(persona.fecha_nacimiento)\"\r\n                        >\r\n                            OK\r\n                        </v-btn>\r\n                        </v-date-picker>\r\n                    </v-menu>\r\n                    <v-checkbox\r\n                        v-model=\"persona.esCliente\"\r\n                        label=\"Es Cliente\"\r\n                    ></v-checkbox>\r\n                    <v-checkbox\r\n                        v-model=\"persona.esProveedor\"\r\n                        label=\"Es Proveedor\"\r\n                    ></v-checkbox>\r\n                    <v-card-actions>\r\n                        <v-btn to=\"/personas\" color=\"blue-grey\" class=\"mr-4\">Cancelar</v-btn>\r\n                        <v-btn type=\"submit\" color=\"indigo\" class=\"mr-4\">Guardar</v-btn>\r\n                    </v-card-actions>\r\n                </form>\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\nlet url = 'http://localhost:3000/personas/'\r\nimport axios from 'axios';\r\nexport default {\r\n    name:'CrearPersona',\r\n    data(){\r\n        return{\r\n            //date: new Date().toISOString().substr(0, 10),\r\n            persona:{\r\n                tipo_identificacion:'',\r\n                identificacion:'',\r\n                nombres:'',\r\n                apellidos:'',\r\n                direccion:'',\r\n                telefono:'',\r\n                email:'',\r\n                fecha_nacimiento: new Date().toISOString().substr(0, 10),\r\n                esCliente:0,\r\n                esProveedor:0,\r\n                estado:'A'\r\n            },\r\n            tipo:[\r\n                {text:'Cedula',value:'C'},\r\n                {text:'Ruc',value:'R'}\r\n            ],\r\n            menu: false,\r\n            //modal: false,\r\n        }\r\n    },\r\n    methods:{\r\n        guardarPersona(){\r\n            let router=this.$router;\r\n            console.log(router);\r\n            let params = this.persona;\r\n            console.log(params);\r\n            console.log(url)\r\n            axios.post(url,params)\r\n            .then(()=>{\r\n                router.push('/personas');\r\n            })\r\n            .catch(error=>{\r\n                console.log(error.response.data);\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/views/personas"}]}