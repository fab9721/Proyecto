:"CrearProductos.vue","sourceRoot":"src/views/productos","sourcesContent":["<template>\r\n    <v-container>\r\n        <v-row>\r\n            <v-col class=\"mb-4\">\r\n                <h1 class=\"display-2 font-weight-bold-mb-3\">Ingreso de productos</h1>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col>\r\n                <form v-on:submit.prevent=\"guardarProducto()\">\r\n                    <v-text-field \r\n                        v-model=\"articulo.codigo\"\r\n                        label=\"Codigo\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"articulo.descripcion\"\r\n                        label=\"Descripcion\"\r\n                        outlined\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-select\r\n                        v-model=\"articulo.categoria_id\"\r\n                        item-value=\"categoria_id\"\r\n                        item-text=\"descripcion\"\r\n                        :items = \"categoria\"\r\n                        label=\"Categoria\"\r\n                        required\r\n                        :rules=\"rules.select\"\r\n                    >\r\n                    </v-select>\r\n                    <v-text-field \r\n                        v-model=\"articulo.stock\"\r\n                        label=\"Stock\"\r\n                        outlined\r\n                        type=\"number\"\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"articulo.precio_unitario\"\r\n                        label=\"Precio\"\r\n                        outlined\r\n                        type=\"number\"\r\n                        prefix=\"$\"\r\n                        required\r\n                    >\r\n                    </v-text-field>\r\n                    <v-radio-group label=\"Graba Iva?\"\r\n                        v-model=\"articulo.graba_iva\"\r\n                    >     \r\n                        <v-radio name=\"iva\" label=\"NO\" :value=\"0\"></v-radio>\r\n                        <v-radio name=\"iva\" label=\"SI\" :value=\"1\"></v-radio>\r\n                    </v-radio-group>\r\n                    <v-text-field \r\n                        v-model=\"articulo.porcentaje_descuento\"\r\n                        label=\"% Descuento\"\r\n                        outlined\r\n                        type=\"number\"\r\n                        prefix=\"%\"\r\n                    >\r\n                    </v-text-field>\r\n                    <v-card-actions>\r\n                        <v-btn to=\"/productos\" color=\"blue-grey\" class=\"mr-4\">Cancelar</v-btn>\r\n                        <v-btn type=\"submit\" color=\"indigo\" class=\"mr-4\">Guardar</v-btn>\r\n                    </v-card-actions>\r\n                </form>\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\nlet url = 'http://localhost:3000/productos/'\r\nlet urlCategoria = 'http://localhost:3000/categorias/all/'\r\nimport axios from 'axios';\r\nexport default {\r\n    name:'CrearProducto',\r\n    created(){\r\n        this.obtenerCategorias();\r\n    },\r\n    data(){\r\n        return{\r\n            articulo:{\r\n                codigo:'',\r\n                descripcion:'',\r\n                categoria_id:null,\r\n                stock:0,\r\n                precio_unitario:0,\r\n                graba_iva:0,\r\n                porcentaje_descuento:null\r\n            },\r\n            categoria:[],\r\n            rules: {\r\n                select: [(v) => !!v || \"Item is required\"],\r\n                select2: [(v) =>  v.length>0 || \"Item is required in select 2\"],\r\n            },\r\n        }\r\n    },\r\n    methods:{\r\n        guardarProducto(){\r\n            let router=this.$router;\r\n            console.log(router);\r\n            let params = this.articulo;\r\n            console.log(params);\r\n            console.log(url)\r\n            axios.post(url,params)\r\n            .then(()=>{\r\n                router.push('/productos');\r\n            })\r\n            .catch(error=>{\r\n                console.log(error.response.data);\r\n            })\r\n        },\r\n        obtenerCategorias(){\r\n            axios.get(urlCategoria)\r\n            .then(response=>{\r\n                this.categoria = response.data.data;\r\n                console.log(this.categoria);\r\n            }).catch((error)=>{\r\n                console.log(error);\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>"]}]}