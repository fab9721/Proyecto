ntarios","sourcesContent":["<template>\r\n    <v-container>\r\n        <v-row>\r\n            <v-col class=\"mb-4\">\r\n                <h1 class=\"display-2 font-weight-bold-mb-3\">Movimiento de Inventario</h1>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col>\r\n                <form>\r\n                    <v-text-field \r\n                        v-model=\"inventario.fecha\"\r\n                        label=\"Fecha\"\r\n                        outlined\r\n                        readonly\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"inventario.tipo\"\r\n                        label=\"Tipo\"\r\n                        outlined\r\n                        readonly\r\n                    >\r\n                    </v-text-field>\r\n                    <v-text-field \r\n                        v-model=\"inventario.bodega\"\r\n                        label=\"Bodega\"\r\n                        outlined\r\n                        readonly\r\n                    >\r\n                    </v-text-field>\r\n                </form>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col class=\"mb-4\">\r\n                <v-card>\r\n                <v-card-title>\r\n                <v-text-field\r\n                    v-model=\"search\"\r\n                    append-icon=\"mdi-magnify\"\r\n                    label=\"Search\"\r\n                    single-line\r\n                    hide-details\r\n                ></v-text-field>\r\n                </v-card-title>\r\n                <v-data-table\r\n                    :headers=\"headers\"\r\n                    :items=\"inventario.detalle\"\r\n                    :search=\"search\"\r\n                ></v-data-table>\r\n            </v-card>\r\n            </v-col>\r\n        </v-row>\r\n        <v-card-actions>\r\n                        <v-btn to=\"/inventarios\" color=\"primary\" class=\"mr-4\">Cancelar</v-btn>\r\n                        <!-- <v-btn type=\"submit\" color=\"indigo\" class=\"mr-4\">Guardar</v-btn> -->\r\n        </v-card-actions>\r\n    </v-container>\r\n</template>\r\n<script>\r\nlet url = 'http://localhost:3000/inventarios/'\r\nimport axios from 'axios';\r\nexport default {\r\n    name:'VerInventario',\r\n    mounted(){\r\n        //OBTENGO TODOS LOS DATOS\r\n        this.movimiento_inventario_id=this.$route.params.id\r\n        //console.log(this.producto_id);\r\n        axios.get(url+this.movimiento_inventario_id)\r\n        .then(response=>{\r\n            this.inventario=response.data[0]\r\n            console.log(this.inventario);\r\n        }).catch((error)=>{\r\n            console.log(\"El error de la peticion\"+error);\r\n        });\r\n    },\r\n    data(){\r\n        return{\r\n            movimiento_inventario_id:null,\r\n            search:'',\r\n            headers: [\r\n            { text: 'Producto', value: 'descripcion' },\r\n            { text: 'Cantidad', value: 'cantidad' },\r\n            { text: 'Precio', value: 'precio_unitario' },\r\n            { text: 'Total', value: 'total' },\r\n            ],\r\n            inventario:{\r\n                fecha:new Date().toISOString().substr(0, 10),\r\n                tipo:'',\r\n                bodega:'',\r\n                detalle:[\r\n                    {\r\n                        descripcion:null,\r\n                        cantidad:null,\r\n                        precio_unitario:null,\r\n                        total:null\r\n                    }\r\n                ]\r\n            },\r\n            \r\n        }\r\n    },\r\n    methods:{\r\n    }\r\n}\r\n</script>"]}]}